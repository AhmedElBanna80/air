{
  "name": "api",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "exports": {
    "./types": "./src/lib/types.ts",
    "./client": "./src/lib/api-client.ts"
  },
  "main": "./dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node --require ./src/register.js dist/index.js",
    "lint": "eslint . --max-warnings 0",
    "type-check": "tsc --noEmit",
