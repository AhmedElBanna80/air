    "@workspace/eslint-config": "workspace:*",
    "@workspace/typescript-config": "workspace:*",
    "csv-parser": "^3.2.0",
    "dotenv": "^16.4.7",
    "dotenv-expand": "^12.0.1",
    "drizzle-orm": "0.29.0",
    "drizzle-zod": "^0.5.1",
    "hono": "^4.7.2",
    "hono-pino": "^0.7.2",
    "papaparse": "^5.4.1",
    "pg": "^8.13.3",
    "pino": "^9.6.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/node": "22.13.5",
    "@types/papaparse": "^5.3.14",
    "@types/pg": "^8.11.0",
    "drizzle-kit": "0.20.0",
    "eslint": "^9.0.0",
    "tsc-alias": "^1.8.10",
    "tsx": "^4.19.3",
    "typescript": "^5"
  }
